{
	"printWidth": 80,
	"trailingComma": "all",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"singleAttributePerLine": true,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^./parser/(.*)$",
		"^./parser",
		"^./constants/(.*)$",
		"^./constants",
		"^./utils/(.*)$",
		"^./utils",
		"^./@types/(.*)$",
		"^./@types",
		"^../(.*)$",
		"^./(.*)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
